import java.util.Scanner;
public class 7.3 {
	final static int m = 100;

	  public static void main(String[] args) {
		int[] inputStorage = new int[m];
		int input;
		Scanner inputScanner = new Scanner(System.in);
    System.out.print("Enter the integers between 1 and " + m + ": ");
		input = inputScanner.nextInt();
		while (input != 0) {
			inputStorage[input - 1]++;
			input = inputScanner.nextInt();
		}

		for (int i = 0; i < 100; i++) {
			if (inputStorage[i] == 1)
				System.out.println(i + 1 + " occurs 1 time");
			else if (inputStorage[i] > 1)
				System.out.println(i + 1 + " occurs " + inputStorage[i]
						+ " times");
		}
	}

}
