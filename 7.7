public class Exercise07_07 {
    public static void main(String[] args) {
        int[] counts = new int[10];
        int ran = 0;
        for (int i = 0; i < 100; i++) {
            ran = (int) (Math.random() * 10);
            counts[ran]++;
        }

        for (int i = 0; i < counts.length; i++) {
            System.out.print(counts[i] + " counts of ");
            System.out.println(i);
        }


    }

}
